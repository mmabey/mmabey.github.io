<?xml version="1.0" encoding="utf-8"?>
<xs:schema
  xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:head="https://mikemabey.com/schema/header_info"
  xmlns:email="https://mikemabey.com/schema/headers"
  targetNamespace="https://mikemabey.com/schema/headers"
  version="0.1"
  elementFormDefault="qualified">

  <xs:annotation>
    <xs:documentation>
      This is the schema file for representing email headers version 0.1

      Most headers defined in this schema come from RFC 4021, but not all. Here is the complete list of RFCs that
      define email headers:
      - RFC  822
      - RFC 1327
      - RFC 1505
      - RFC 2076
      - RFC 2156
      - RFC 2298
      - RFC 2369
      - RFC 2822
      - RFC 2919
      - RFC 3282
      - RFC 3297
      - RFC 3458
      - RFC 3789
      - RFC 3834
      - RFC 3865
      - RFC 4021
      - RFC 5064
      - RFC 5321
      - RFC 5322
      - RFC 5337
      - RFC 5504
      - RFC 5518
      - RFC 5703
      - RFC 6017
      - RFC 6109
      - RFC 6376
      - RFC 6477
      - RFC 6758
      - RFC 6854
      - RFC 6857
      - RFC 7208
      - RFC 7259
      - RFC 7293
      - RFC 7444
      - RFC 7601
      - RFC 7681
      - RFC 7912
      - RFC 8058

    </xs:documentation>
  </xs:annotation>

  <!--   Import/Include Common Elements, MIME Header Elements, Header Info Schema   -->
  <xs:include schemaLocation="https://mikemabey.com/schema/mime_headers.xsd"/>
  <xs:include schemaLocation="https://mikemabey.com/schema/headers_common.xsd"/>
  <xs:import namespace="https://mikemabey.com/schema/header_info" schemaLocation="https://mikemabey.com/schema/header_info.xsd"/>

  <!--   Custom Types   -->

  <xs:complexType name="address_field">
    <xs:sequence>
      <xs:element name="address" minOccurs="1" maxOccurs="unbounded">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="alias" type="xs:string" minOccurs="0" maxOccurs="1"/>
            <xs:element name="email" type="xs:string" minOccurs="1" maxOccurs="1"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="date_field">
    <xs:sequence>
      <xs:element name="year" type="xs:integer" maxOccurs="1"/>
      <xs:element name="month" type="xs:integer" maxOccurs="1"/>
      <xs:element name="day" type="xs:integer" maxOccurs="1"/>
      <xs:element name="hour" type="xs:integer" maxOccurs="1"/>
      <xs:element name="min" type="xs:integer" maxOccurs="1"/>
      <xs:element name="sec" type="xs:integer" maxOccurs="1"/>
      <xs:element name="date_str" type="xs:string" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:simpleType name="downgraded_field">
    <xs:restriction base="head:string"/>
  </xs:simpleType>

  <xs:complexType name="keywords_field">
    <xs:sequence>
      <xs:element name="word" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="received_field">
    <xs:sequence>
      <xs:element name="from" type="xs:string"/>
      <xs:element name="by" type="xs:string"/>
      <xs:element name="via" type="xs:string"/>
      <xs:element name="with" type="xs:string"/>
      <xs:element name="id" type="xs:string"/>
      <xs:element name="for" type="xs:string"/>
      <xs:element name="other" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="reference_field">
    <xs:sequence>
      <xs:element name="ref" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>


  <!--   Permanent Message Header Field Elements   -->
  <!-- See https://www.iana.org/assignments/message-headers/message-headers.xhtml -->

  <xs:element name="Accept-Language" type="head:language_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 3282" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Language(s) for auto-responses

        Indicates a language that the message sender requests to be used for responses.  Accept-language was not
        designed for email but has been considered useful as input to the generation of automatic replies.  Some
        problems have been noted concerning its use with email, including but not limited to determination of the email
        address to which it refers; cost and lack of effective internationalization of email responses; interpretation
        of language subtags; and determining what character set encoding should be used.

        Specification document(s): RFC 4021, RFC 3282
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Alternate-Recipient" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Controls forwarding to alternate recipients

        Controls whether this message may be forwarded to an alternate recipient, such as a postmaster, if delivery to
        the intended recipient is not possible.  Default: Allowed.  RFC 2156 (MIXER), not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Archived-At" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5064" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Direct link to the archived form of an individual email message

        Refers to a single message at an archived location.  This provides quick access to the location of a mailing
        list message in the list archive.  It can also be used independently of mailing lists, for example in connection
        with legal requirements to archive certain messages.

        Specification document(s): RFC 5064
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Authentication-Results" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="7601" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Message authentication results

        Presents the results of a message authentication effort in a machine-readable format.  The intent of the header
        field is to create a place to collect such data when message authentication mechanisms are in use so that a Mail
        User Agent (MUA) and downstream filters can make filtering decisions and/ or provide a recommendation to the
        user as to the validity of the message's origin and possibly the safety and integrity of its content.

        Specification document(s): RFC 7601
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Auto-Submitted" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="3834" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        The message was sent by an automated process

        The purpose of the Auto-Submitted header field is to indicate that the message was originated by an automatic
        process, or an automatic responder, rather than by a human; and to facilitate automatic filtering of messages
        from signal paths for which automatically generated messages and automatic responses are not desirable.

        Specification document(s): RFC 3834
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Autoforwarded" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Automatically forwarded indicator

        Has been automatically forwarded.  RFC 2156 (MIXER), not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Autosubmitted" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Automatically submitted indicator

        Has been automatically submitted.  RFC 2156 (MIXER); not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Bcc" type="email:address_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5322 4021 822 2822" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Blind-carbon-copy recipient mailbox

        Contains addresses of recipients of the message whose addresses are not to be revealed to other recipients of
        the message.  Defined as standard by RFC 822.

        Specification document(s): RFC 5322, RFC 4021, RFC 822, RFC 2822
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Cc" type="email:address_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5322 4021 2822 822" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Carbon-copy recipient mailbox

        Contains the addresses of others who are to receive the message, though the content of the message may not be
        directed at them.  Defined as standard by RFC 822.

        Specification document(s): RFC 5322, RFC 4021, RFC 2822, RFC 822
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Comments" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5322 4021 822 2822" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Additional comments about the message

        Contains any additional comments on the text of the body of the message.  Warning: Some mailers will not show
        this field to recipients.  Defined as standard by RFC 822.

        Specification document(s): RFC 5322, RFC 4021, RFC 822, RFC 2822
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Content-Identifier" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 1327" type="permanent" status="obsolete"/></xs:appinfo>
      <xs:documentation>
        Message content identifier

        A text string that identifies the content of a message.  Introduced by RFC 1327 and subsequently changed by RFC
        2156 to avoid confusion with MIME defined fields.  Gateways that reverse map may support the old field.

        Specification document(s): RFC 4021, RFC 1327
        Type: Permanent
        Status: obsolete
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Content-Return" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 1327" type="permanent" status="obsolete"/></xs:appinfo>
      <xs:documentation>
        Return content on non-delivery?

        Indicates whether the content of a message is to be returned with non-delivery notifications.  Introduced by RFC
        1327 and subsequently changed by RFC 2156 to avoid confusion with MIME defined fields.

        Specification document(s): RFC 4021, RFC 1327
        Type: Permanent
        Status: obsolete
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Conversion" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Conversion allowed?

        The body of this message may not be converted from one character set to another.  Values: prohibited and
        allowed.  RFC 2156 (MIXER); not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Conversion-With-Loss" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Lossy conversion allowed?

        The body of this message may not be converted from one character set to another if information will be lost.
        Values: prohibited and allowed.  RFC 2156 (MIXER); not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="DL-Expansion-History" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Trace of distribution lists passed

        Trace of distribution lists passed. (MIXER X.400 mapping; not for general use.)

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Date" type="email:date_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5322 4021 822 2822" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Message date and time

        Specifies the date and time at which the creator of the message indicated that the message was complete and
        ready to enter the mail delivery system.  Defined as standard by RFC 822.

        Specification document(s): RFC 5322, RFC 4021, RFC 822, RFC 2822
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Deferred-Delivery" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Deferred delivery information

        Provides information about deferred delivery service to the recipient.  RFC 2156 (MIXER); not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Delivery-Date" type="email:date_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Message delivery time

        The time when a message was delivered to its recipient.  RFC 2156 (MIXER); not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Discarded-X400-IPMS-Extensions" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        X.400 IPM extensions discarded

        Can be used in Internet mail to indicate X.400 IPM extensions that could not be mapped to Internet mail format.
        RFC 2156 (MIXER); not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Discarded-X400-MTS-Extensions" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        X.400 MTS extensions discarded

        Can be used in Internet mail to indicate X.400 MTS extensions that could not be mapped to Internet mail format.
        RFC 2156 (MIXER); not for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Disclose-Recipients" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2156" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Disclose names of other recipients?

        Tells whether recipients are to be told the names of other recipients of the same message.  This is primarily an
        X.400 facility.  In X.400, this is an envelope attribute and refers to disclosure of the envelope recipient
        list.  Disclosure of other recipients is done in Internet mail via the To:, cc:, and bcc: header fields.  Not
        for general use.

        Specification document(s): RFC 4021, RFC 2156
        Type: Permanent
        Status: standards-track
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Disposition-Notification-Options" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2298" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Disposition notification options

        For optional modifiers on disposition notification requests.

        Specification document(s): RFC 4021, RFC 2298
        Type: Permanent
        Status:
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Disposition-Notification-To" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="4021 2298" type="permanent" status="standards-track"/></xs:appinfo>
      <xs:documentation>
        Mailbox for sending disposition notification

        Indicates that the sender wants a disposition notification when this message is received (read, processed, etc.)
        by its recipients.

        Specification document(s): RFC 4021, RFC 2298
        Type: Permanent
        Status:
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="DKIM-Signature" type="head:string">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="6376" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation> The signature of the email

        DomainKeys Identified Mail (DKIM) permits a person, role, or organization that owns the signing domain to claim
        some responsibility for a message by associating the domain with the message.  This can be an author's
        organization, an operational relay, or one of their agents.  DKIM separates the question of the identity of the
        Signer of the message from the purported author of the message.  Assertion of responsibility is validated
        through a cryptographic signature and by querying the Signer's domain directly to retrieve the appropriate
        public key.  This header field contains all of the signature and key-fetching data.

        Specification document(s): RFC 6376
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-Bcc" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5504 6857" type="permanent" status="obsolete"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 Bcc header

        If an envelope address or header field contains non-ASCII characters, the message cannot be delivered unless
        every system in the delivery path supports UTF8SMTP.  Fields starting with "Downgraded-" preserve those original
        envelope and mail header field values that contain UTF-8 characters.

        Specification document(s): RFC 5504, RFC 6857
        Type: Permanent
        Status: obsolete
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-Cc" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5504 6857" type="permanent" status="obsolete"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 Cc header

        If an envelope address or header field contains non-ASCII characters, the message cannot be delivered unless
        every system in the delivery path supports UTF8SMTP.  Fields starting with "Downgraded-" preserve those original
        envelope and mail header field values that contain UTF-8 characters.

        Specification document(s): RFC 5504, RFC 6857
        Type: Permanent
        Status: obsolete
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-Disposition-Notification-To" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5504 6857" type="permanent" status="obsolete"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 Disposition-Notification-To header

        If an envelope address or header field contains non-ASCII characters, the message cannot be delivered unless
        every system in the delivery path supports UTF8SMTP.  Fields starting with "Downgraded-" preserve those original
        envelope and mail header field values that contain UTF-8 characters.

        Specification document(s): RFC 5504, RFC 6857
        Type: Permanent
        Status: obsolete
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-Final-Recipient" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="6857" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 Final-Recipient header

        If an envelope address or header field contains non-ASCII characters, the message cannot be delivered unless
        every system in the delivery path supports UTF8SMTP.  Fields starting with "Downgraded-" preserve those original
        envelope and mail header field values that contain UTF-8 characters.

        Specification document(s): RFC 6857
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-From" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5504 6857" type="permanent" status="obsolete"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 From header

        If an envelope address or header field contains non-ASCII characters, the message cannot be delivered unless
        every system in the delivery path supports UTF8SMTP.  Fields starting with "Downgraded-" preserve those original
        envelope and mail header field values that contain UTF-8 characters.

        Specification document(s): RFC 5504, RFC 6857
        Type: Permanent
        Status: obsolete
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-In-Reply-To" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="6857" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 In-Reply-To header

        If a header field contains non-ASCII strings, a POP or IMAP server cannot deliver internationalized messages to
        legacy clients that do not send UTF8 commands or have UTF8 capability.  Fields starting with "Downgraded-"
        preserve the information that appeared in the original header fields.

        Specification document(s): RFC 6857
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-Mail-From" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5504 6857" type="permanent" status="obsolete"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 Mail-From header

        If an envelope address or header field contains non-ASCII characters, the message cannot be delivered unless
        every system in the delivery path supports UTF8SMTP.  Fields starting with "Downgraded-" preserve those original
        envelope and mail header field values that contain UTF-8 characters.

        Specification document(s): RFC 5504, RFC 6857
        Type: Permanent
        Status: obsolete
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-Message-Id" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="6857" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 Message-Id header

        If a header field contains non-ASCII strings, a POP or IMAP server cannot deliver internationalized messages to
        legacy clients that do not send UTF8 commands or have UTF8 capability.  Fields starting with "Downgraded-"
        preserve the information that appeared in the original header fields.

        Specification document(s): RFC 6857
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-Original-Recipient" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="6857" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 Original-Recipient header

        If a header field contains non-ASCII strings, a POP or IMAP server cannot deliver internationalized messages to
        legacy clients that do not send UTF8 commands or have UTF8 capability.  Fields starting with "Downgraded-"
        preserve the information that appeared in the original header fields.

        Specification document(s): RFC 6857
        Type: Permanent
        Status: standard
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-Rcpt-To" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="5504 6857" type="permanent" status="obsolete"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 Rcpt-To header

        If an envelope address or header field contains non-ASCII characters, the message cannot be delivered unless
        every system in the delivery path supports UTF8SMTP.  Fields starting with "Downgraded-" preserve those original
        envelope and mail header field values that contain UTF-8 characters.

        Specification document(s): RFC 5504, RFC 6857
        Type: Permanent
        Status: obsolete
      </xs:documentation>
    </xs:annotation>
  </xs:element>

  <xs:element name="Downgraded-References" type="email:downgraded_field">
    <xs:annotation>
      <xs:appinfo><head:header protocol="mail" rfc="6857" type="permanent" status="standard"/></xs:appinfo>
      <xs:documentation>
        Original UTF-8 References header

        If a header field contains non-ASCII strings, a POP or IMAP server cannot deliver internationalized messages to
